#include<stdio.h>
int main(){
    int n, f0=0,f1=1,f2;
    scanf("%d", &n);
    for(int i = 0;i<n;i++){
        if(i==0){
            printf("0");
        }else if(i==1){
            printf(" 1");
        }
        else{
            f2= f1+f0;
            printf(" %d",f2);
            f0 = f1;
            f1=f2;
        }
        
    }
    printf("\n");
}
