#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

  int n, num, first,last,digit =0;
    scanf("%d", &n);
    num = n;
    last = num%10;
    while(num>=10){
        num = num/10;
        digit++;
    }
    first = num;
    int pow10 = (int)pow(10,digit);
    int mid = (n%pow10/10);
    int result = last*pow10 + mid*10+first;
    printf("%d", result);
    return 0;
}
