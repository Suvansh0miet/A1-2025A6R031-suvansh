#include <stdio.h>

int main() {
    int amount;
    scanf("%d", &amount);

    int denominations[] = {500, 100, 50, 20, 10, 5, 2, 1};
    int count;

    for (int i = 0; i < 8; i++) {
        count = amount / denominations[i];
        if (count > 0) {
            printf("%d: %d\n", denominations[i], count);
            amount %= denominations[i];
        }
    }

    return 0;
}
