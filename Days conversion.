#include <stdio.h>
int main(){
    int total_days;
    scanf("%d", &total_days);
    
    int years = total_days / 365;
    int remaining_days = total_days % 365;
    
    int weeks = remaining_days / 7;
    int days = remaining_days % 7;
    printf("Years: %d\n", years);
    printf("Weeks: %d\n", weeks);
    printf("Days: %d\n", days);
    return 0;
}
